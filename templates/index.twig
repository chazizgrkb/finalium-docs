{% extends "_layout.twig" %}

{% block title %}Home{% endblock %}

	{% block content %}
<div class="row">
	<div class="col-md-8">
		<div class="panel panel-default">
			<div class="panel-heading">Featured Videos</div>
			<div class="panel-body">
				{% for video in videos %}
					{{ video_box(video) }}
